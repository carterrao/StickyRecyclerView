<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rzc.stickyrecyclerview">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".demo.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".demo.ImageActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".demo.WebActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".demo.simplelistview.SimpleListViewActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".demo.headerfooterlistview.HeaderFooterListViewActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".demo.scrollheaderfooterlistview.ScrollHeaderFooterListViewActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".demo.expandablelistview.ExpandableListViewActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".demo.completeexpandablelistview.CompleteExpandableListViewActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>